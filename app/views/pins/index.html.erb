<!--<p id="notice"><%= notice %></p>-->

<%= render '/home/jumbo' %>

<h4>Listing of Pins <%= link_to 'New Post', new_pin_path, class: "center" %></h4>

  
<div id="pins" class="transitions-enabled infinite-scroll clearfix">
    <% @pins.each do |pin| %>

        <div class="box">
            <div class="panel panel-default">
                <%= link_to image_tag(pin.image.url(:medium)), pin %><br>
                <div class="panel-body">      
                    <%= pin.description %> <br>
                    <strong><em>posted by: <%= pin.user.name if pin.user %></em></strong><br><br>
                    <% if pin.user == current_user %>    
                        <!--<%= link_to 'Show', pin %>-->
                        <%= link_to 'Edit', edit_pin_path(pin) %> | 
                        <%= link_to 'Delete', pin, method: :delete, data: { confirm: 'Are you sure?' } %><br>
                    <% end %>
                </div>
            </div>
        </div>

    <% end %>
    
</div>























